{
  "name": "finderx",
  "type": "module",
  "version": "1.0.2",
  "description": "precise query node by css selector ",
  "repository": "liuzhaodong/finderx",
  "homepage": "https://github.com/liuzhaodong/finderx",
  "author": "liuzhaodong <liuzhaodong@gmail.com>",
  "license": "MIT",
  "keywords": [
    "css",
    "selector",
    "generator"
  ],
  "main": "finderx.js",
  "types": "finderx.d.ts",
  "files": [
    "*.ts",
    "*.js"
  ],
  "scripts": {
    "start": "tsc -w",
    "test": "tsc && ava --verbose --timeout 3m",
    "prepare": "tsc",
    "release": "release-it --access public",
    "size": "minify finderx.js --sourceType module | gzip-size"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "babel-minify": "*",
    "browser-env": "^3.3.0",
    "esm": "^3.2.25",
    "gzip-size-cli": "*",
    "keyv": "^4.5.0",
    "release-it": "^13.6.1",
    "ts-node": "^10.2.1",
    "typescript": "3.9.3"
  },
  "ava": {
    "require": [
      "./test/helpers/setup-browser-env.js"
    ],
    "extensions": {
      "ts": "module",
      "js": true
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
